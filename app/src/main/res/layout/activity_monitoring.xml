<?xml version="1.0" encoding="utf-8"?>
<androidx.swiperefreshlayout.widget.SwipeRefreshLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/containerSwipe">
    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:background="#E0E0E0"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbar"
                android:background="@color/colorPrimaryDark"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="MissingConstraints">
                <LinearLayout
                    android:layout_marginTop="@dimen/_10sdp"
                    android:paddingBottom="@dimen/_10sdp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/backButton"
                            android:layout_width="@dimen/_25sdp"
                            android:layout_height="@dimen/_25sdp"
                            android:layout_gravity="center"
                            android:onClick="back"
                            android:tint="@color/white"
                            android:src="@drawable/ic_arrow_back"
                            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck,UseAppTint" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_10sdp"
                            android:layout_gravity="center"
                            android:fontFamily="@font/robotobold"
                            android:textSize="@dimen/_15sdp"
                            android:text="Live Monitoring"
                            android:textColor="@color/white" />
                    </LinearLayout>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout
                android:layout_below="@+id/toolbar"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <ScrollView
                    android:id="@+id/scrollView"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="vertical">
                        <LinearLayout
                            android:background="@drawable/white_rect"
                            android:layout_marginLeft="@dimen/_15sdp"
                            android:layout_marginRight="@dimen/_15sdp"
                            android:layout_marginBottom="@dimen/_5sdp"
                            android:layout_height="wrap_content"
                            android:elevation="@dimen/_2sdp"
                            android:id="@+id/lnSlider"
                            android:padding="@dimen/_10sdp"
                            android:orientation="vertical"
                            android:layout_width="match_parent">
                            <LinearLayout
                                android:layout_marginBottom="@dimen/_10sdp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                tools:ignore="MissingConstraints">
                                <ImageView
                                    android:id="@+id/ivSocket"
                                    android:layout_width="@dimen/_15sdp"
                                    android:layout_height="@dimen/_15sdp"
                                    android:src="@drawable/circle_danger"/>
                                <TextView
                                    android:layout_marginLeft="@dimen/_5sdp"
                                    android:id="@+id/tvSocket"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Server Disconnected"
                                    android:textStyle="bold"
                                    android:textColor="@color/black"/>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">
                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"
                                    android:text="ID Alat"
                                    android:textSize="@dimen/_9sdp"
                                    android:textColor="@color/black"
                                    android:fontFamily="@font/robotolight"/>
                                <TextView
                                    android:id="@+id/tvIdAlat"
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"
                                    android:text="..."
                                    android:gravity="right"
                                    android:textSize="@dimen/_9sdp"
                                    android:textColor="@color/black"
                                    android:fontFamily="@font/robotobold"/>
                            </LinearLayout>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_1sdp"
                                android:background="@drawable/dashed_line"
                                android:layout_margin="@dimen/_5sdp"/>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">
                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"
                                    android:text="Status Paket"
                                    android:textSize="@dimen/_9sdp"
                                    android:textColor="@color/black"
                                    android:fontFamily="@font/robotolight"/>
                                <TextView
                                    android:id="@+id/tvStatus"
                                    android:layout_width="0dp"
                                    android:layout_weight="0.5"
                                    android:gravity="center"
                                    android:layout_height="wrap_content"
                                    android:paddingTop="@dimen/_5sdp"
                                    android:paddingBottom="@dimen/_5sdp"
                                    android:paddingLeft="@dimen/_15sdp"
                                    android:paddingRight="@dimen/_15sdp"
                                    android:fontFamily="@font/robotobold"
                                    android:textColor="@drawable/txt_alert_selector"
                                    android:background="@drawable/bg_alert_selector"
                                    android:textSize="@dimen/_10sdp"/>
                            </LinearLayout>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_1sdp"
                                android:background="@drawable/dashed_line"
                                android:layout_margin="@dimen/_5sdp"/>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">
                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"
                                    android:text="Status Alat"
                                    android:textSize="@dimen/_9sdp"
                                    android:textColor="@color/black"
                                    android:fontFamily="@font/robotolight"/>
                                <TextView
                                    android:id="@+id/tvStatusAlat"
                                    android:layout_width="0dp"
                                    android:layout_weight="0.5"
                                    android:gravity="center"
                                    android:layout_height="wrap_content"
                                    android:paddingTop="@dimen/_5sdp"
                                    android:paddingBottom="@dimen/_5sdp"
                                    android:paddingLeft="@dimen/_15sdp"
                                    android:paddingRight="@dimen/_15sdp"
                                    android:fontFamily="@font/robotobold"
                                    android:textColor="@drawable/txt_alert_selector"
                                    android:background="@drawable/bg_alert_selector"
                                    android:textSize="@dimen/_10sdp"/>
                            </LinearLayout>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_1sdp"
                                android:background="@drawable/dashed_line"
                                android:layout_margin="@dimen/_5sdp"/>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">
                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"
                                    android:text="Paket"
                                    android:textSize="@dimen/_9sdp"
                                    android:textColor="@color/black"
                                    android:fontFamily="@font/robotolight"/>
                                <TextView
                                    android:id="@+id/tvPeriode"
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"
                                    android:text="..."
                                    android:gravity="right"
                                    android:textSize="@dimen/_9sdp"
                                    android:textColor="@color/black"
                                    android:fontFamily="@font/robotobold"/>
                            </LinearLayout>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_1sdp"
                                android:background="@drawable/dashed_line"
                                android:layout_margin="@dimen/_5sdp"/>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">
                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"
                                    android:text="Tanggal Mulai"
                                    android:textSize="@dimen/_9sdp"
                                    android:textColor="@color/black"
                                    android:fontFamily="@font/robotolight"/>
                                <TextView
                                    android:id="@+id/tvTanggalMulai"
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"
                                    android:text="..."
                                    android:gravity="right"
                                    android:textSize="@dimen/_9sdp"
                                    android:textColor="@color/black"
                                    android:fontFamily="@font/robotobold"/>
                            </LinearLayout>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_1sdp"
                                android:background="@drawable/dashed_line"
                                android:layout_margin="@dimen/_5sdp"/>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">
                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"
                                    android:text="Berakhir Pada"
                                    android:textSize="@dimen/_9sdp"
                                    android:textColor="@color/black"
                                    android:fontFamily="@font/robotolight"/>
                                <TextView
                                    android:id="@+id/tvTanggalExpired"
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"
                                    android:text="..."
                                    android:gravity="right"
                                    android:textSize="@dimen/_9sdp"
                                    android:textColor="@color/black"
                                    android:fontFamily="@font/robotobold"/>
                            </LinearLayout>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_1sdp"
                                android:background="@drawable/dashed_line"
                                android:layout_margin="@dimen/_5sdp"/>
                            <Button
                                android:id="@+id/btnCekAlat"
                                style="?android:attr/borderlessButtonStyle"
                                android:layout_width="@dimen/_120sdp"
                                android:layout_height="@dimen/_30sdp"
                                android:layout_marginTop="@dimen/_10sdp"
                                android:layout_marginBottom="@dimen/_10sdp"
                                android:text="Cek Alat"
                                android:fontFamily="@font/robotomedium"
                                android:textAllCaps="false"
                                android:background="@drawable/btn_hover_primary"
                                android:textColor="#FFFFFF"/>
                        </LinearLayout>



                        <LinearLayout
                            android:layout_marginTop="@dimen/_10sdp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <LinearLayout
                                android:id="@+id/lnSensor1"
                                android:background="@drawable/white_rect"
                                android:layout_marginLeft="@dimen/_15sdp"
                                android:layout_marginRight="@dimen/_5sdp"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/_10sdp"
                                android:orientation="vertical"
                                android:layout_width="0dp"
                                android:layout_weight="1">
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">
                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_weight="1"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical">
                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:orientation="horizontal">
                                            <TextView
                                                android:id="@+id/tvIn1"
                                                android:layout_width="0dp"
                                                android:layout_weight="1"
                                                android:layout_height="wrap_content"
                                                android:text="Modul Switch"
                                                android:textColor="@color/black"
                                                android:textSize="@dimen/_14sdp"
                                                android:fontFamily="@font/robotomedium"/>
                                            <ImageView
                                                android:layout_gravity="center"
                                                android:id="@+id/ivEditSensor1"
                                                android:layout_width="@dimen/_15sdp"
                                                android:layout_height="@dimen/_15sdp"
                                                android:src="@drawable/edit"/>
                                        </LinearLayout>

                                        <LinearLayout
                                            android:layout_marginTop="@dimen/_10sdp"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:orientation="horizontal">
                                            <ImageView
                                                android:id="@+id/ivSensor1"
                                                android:layout_width="@dimen/_15sdp"
                                                android:layout_height="@dimen/_15sdp"
                                                android:src="@drawable/circle_success"/>
                                            <TextView
                                                android:layout_marginLeft="@dimen/_5sdp"
                                                android:id="@+id/tvSensor1"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:text="Normal"
                                                android:fontFamily="@font/robotolight"
                                                android:textColor="#000"/>
                                        </LinearLayout>
                                    </LinearLayout>
                                </LinearLayout>
                                <Switch
                                    android:layout_marginTop="@dimen/_10sdp"
                                    android:id="@+id/swIn1"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Disable" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/lnSensor2"
                                android:background="@drawable/white_rect"
                                android:layout_marginLeft="@dimen/_5sdp"
                                android:layout_marginRight="@dimen/_15sdp"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/_10sdp"
                                android:orientation="vertical"
                                android:layout_width="0dp"
                                android:layout_weight="1">
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">
                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_weight="1"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical">
                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:orientation="horizontal">
                                            <TextView
                                                android:id="@+id/tvIn2"
                                                android:layout_width="0dp"
                                                android:layout_weight="1"
                                                android:layout_height="wrap_content"
                                                android:text="Modul Ohm"
                                                android:textColor="@color/black"
                                                android:textSize="@dimen/_14sdp"
                                                android:fontFamily="@font/robotomedium"/>
                                            <ImageView
                                                android:layout_gravity="center"
                                                android:id="@+id/ivEditSensor2"
                                                android:layout_width="@dimen/_15sdp"
                                                android:layout_height="@dimen/_15sdp"
                                                android:src="@drawable/edit"/>
                                        </LinearLayout>
                                        <LinearLayout
                                            android:layout_marginTop="@dimen/_10sdp"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:orientation="horizontal">
                                            <ImageView
                                                android:id="@+id/ivSensor2"
                                                android:layout_width="@dimen/_15sdp"
                                                android:layout_height="@dimen/_15sdp"
                                                android:src="@drawable/circle_success"/>
                                            <TextView
                                                android:layout_marginLeft="@dimen/_5sdp"
                                                android:id="@+id/tvSensor2"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:text="Normal"
                                                android:fontFamily="@font/robotolight"
                                                android:textColor="#000"/>
                                        </LinearLayout>
                                    </LinearLayout>
                                </LinearLayout>
                                <Switch
                                    android:layout_marginTop="@dimen/_10sdp"
                                    android:id="@+id/swIn2"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Disable" />
                            </LinearLayout>

                        </LinearLayout>


                        <LinearLayout
                            android:layout_marginTop="@dimen/_10sdp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <LinearLayout
                                android:id="@+id/lnSensor3"
                                android:background="@drawable/white_rect"
                                android:layout_marginLeft="@dimen/_15sdp"
                                android:layout_marginRight="@dimen/_5sdp"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/_10sdp"
                                android:orientation="vertical"
                                android:layout_width="0dp"
                                android:layout_weight="1">
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">
                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_weight="1"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical">
                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:orientation="horizontal">
                                            <TextView
                                                android:id="@+id/tvIn3"
                                                android:layout_width="0dp"
                                                android:layout_weight="1"
                                                android:layout_height="wrap_content"
                                                android:text="Modul RF"
                                                android:textColor="@color/black"
                                                android:textSize="@dimen/_14sdp"
                                                android:fontFamily="@font/robotomedium"/>
                                            <ImageView
                                                android:layout_gravity="center"
                                                android:id="@+id/ivEditSensor3"
                                                android:layout_width="@dimen/_15sdp"
                                                android:layout_height="@dimen/_15sdp"
                                                android:src="@drawable/edit"/>
                                        </LinearLayout>
                                        <LinearLayout
                                            android:layout_marginTop="@dimen/_10sdp"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:orientation="horizontal">
                                            <ImageView
                                                android:id="@+id/ivSensor3"
                                                android:layout_width="@dimen/_15sdp"
                                                android:layout_height="@dimen/_15sdp"
                                                android:src="@drawable/circle_success"/>
                                            <TextView
                                                android:layout_marginLeft="@dimen/_5sdp"
                                                android:id="@+id/tvSensor3"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:text="Normal"
                                                android:fontFamily="@font/robotolight"
                                                android:textColor="#000"/>
                                        </LinearLayout>
                                    </LinearLayout>
                                </LinearLayout>
                                <Switch
                                    android:layout_marginTop="@dimen/_10sdp"
                                    android:id="@+id/swIn3"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Disable" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/lnAlarm"
                                android:background="@drawable/white_rect"
                                android:layout_marginLeft="@dimen/_5sdp"
                                android:layout_marginRight="@dimen/_15sdp"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/_10sdp"
                                android:orientation="vertical"
                                android:layout_width="0dp"
                                android:layout_weight="1">
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">
                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_weight="1"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical">
                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:orientation="horizontal">
                                            <TextView
                                                android:id="@+id/tvAlarm"
                                                android:layout_width="0dp"
                                                android:layout_weight="1"
                                                android:layout_height="wrap_content"
                                                android:text="Alarm"
                                                android:textColor="@color/black"
                                                android:textSize="@dimen/_14sdp"
                                                android:fontFamily="@font/robotomedium"/>
                                            <ImageView
                                                android:layout_gravity="center"
                                                android:id="@+id/ivEditAlarm"
                                                android:layout_width="@dimen/_15sdp"
                                                android:layout_height="@dimen/_15sdp"
                                                android:src="@drawable/edit"/>
                                        </LinearLayout>
                                        <Switch
                                            android:layout_marginTop="@dimen/_10sdp"
                                            android:id="@+id/swMode"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:text="Otomatis"
                                            android:checked="true"/>
                                        <TextView
                                            android:visibility="gone"
                                            android:id="@+id/tvDelay"
                                            android:layout_marginTop="@dimen/_5sdp"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:text="1500"
                                            android:textSize="@dimen/_9sdp"
                                            android:textColor="@color/black"
                                            android:fontFamily="@font/robotolight"/>
                                        <Button
                                            android:id="@+id/btnAlarm"
                                            style="?android:attr/borderlessButtonStyle"
                                            android:layout_width="match_parent"
                                            android:layout_height="@dimen/_30sdp"
                                            android:layout_marginTop="@dimen/_10sdp"
                                            android:layout_marginBottom="@dimen/_10sdp"
                                            android:text="Hidupkan Alarm"
                                            android:fontFamily="@font/robotomedium"
                                            android:textAllCaps="false"
                                            android:background="@drawable/btn_hover_primary"
                                            android:textColor="#FFFFFF"/>
                                    </LinearLayout>
                                </LinearLayout>
                            </LinearLayout>

                        </LinearLayout>



                        <LinearLayout
                            android:layout_marginTop="@dimen/_15sdp"
                            android:background="@drawable/white_rect"
                            android:layout_marginLeft="@dimen/_15sdp"
                            android:layout_marginRight="@dimen/_15sdp"
                            android:layout_marginBottom="@dimen/_100sdp"
                            android:layout_height="wrap_content"
                            android:elevation="@dimen/_2sdp"
                            android:padding="@dimen/_10sdp"
                            android:orientation="vertical"
                            android:layout_width="match_parent">
                            <LinearLayout
                                android:layout_marginBottom="@dimen/_10sdp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                tools:ignore="MissingConstraints">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Last Log"
                                    android:textStyle="bold"
                                    android:textColor="@color/black"/>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">
                                <TextView
                                    android:id="@+id/tvLogIn1"
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"
                                    android:text="In 1"
                                    android:textSize="@dimen/_9sdp"
                                    android:textColor="@color/black"
                                    android:fontFamily="@font/robotolight"/>
                                <TextView
                                    android:id="@+id/tvLogValIn1"
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"
                                    android:text="..."
                                    android:gravity="right"
                                    android:textSize="@dimen/_9sdp"
                                    android:textColor="@color/black"
                                    android:fontFamily="@font/robotobold"/>
                            </LinearLayout>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_1sdp"
                                android:background="@drawable/dashed_line"
                                android:layout_margin="@dimen/_5sdp"/>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">
                                <TextView
                                    android:id="@+id/tvLogIn2"
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"
                                    android:text="In 2"
                                    android:textSize="@dimen/_9sdp"
                                    android:textColor="@color/black"
                                    android:fontFamily="@font/robotolight"/>
                                <TextView
                                    android:id="@+id/tvLogValIn2"
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"
                                    android:text="..."
                                    android:gravity="right"
                                    android:textSize="@dimen/_9sdp"
                                    android:textColor="@color/black"
                                    android:fontFamily="@font/robotobold"/>
                            </LinearLayout>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_1sdp"
                                android:background="@drawable/dashed_line"
                                android:layout_margin="@dimen/_5sdp"/>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">
                                <TextView
                                    android:id="@+id/tvLogIn3"
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"
